class HelloWorld {
    public static int fibo(int n ){
        int result=0;
        if(n==0||n==1){
            return n;
        }
        else{
         result = fibo(n-1)+fibo(n-2);
        }
        return result;
    }
    public static void main(String[] args) {
        int n =5;
        int m = fibo(n);
        System.out.println(m);
    }
}
