// Use this editor to write, compile and run your Java code online

class HelloWorld {
    public static int fact(int n){
        if(n==0||n==1){
            return 1 ;
        }
        else{
            int result = n*fact(n-1);
            return result;
        }
    }
    public static void main(String[] args) {
        int n =5;
        int m = fact(n);
        System.out.println("Try programiz.pro"+m);
    }
}
